<strong><%= model.full_name %></strong>
Age: <%= Date.today.year - model.date_of_birth.year %>
Height: <%= model.height %>
Shoes: <%= model.shoes %>
Hair: <%= model.hair %>
Eyes: <%= model.eyes %>
<% if model.is_a? Woman %>
  Breast: <%= model.bust %>
  Waist: <%= model.waist %>
  Heaps: <%= model.heaps %>
<% end %>